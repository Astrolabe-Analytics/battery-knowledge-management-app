  doi          = {10.1149/1945-7111/ab9ee8},
  url          = {https://www.osti.gov/biblio/1660905},
  journal      = {Journal of the Electrochemical Society (Online)},
  issn         = {ISSN 1945-7111},
  number       = {11},
  volume       = {167},
  place        = {United States},
  publisher    = {IOP Publishing},
  year         = {2020},
  month        = {07}}

</code>
